{
    "name": "blackcms/cms",
    "description": "BlackCMS is a modular and user friendly CMS (Content Management System) based on Laravel 9.x",
    "version": "1.1.0",
    "homepage": "http://blackcms.pyor.team",
    "keywords": [
        "laravel",
        "cms"
    ],
    "support": {
        "source": "https://github.com/blackcms/cms",
        "issues": "https://github.com/blackcms/cms/issues"
    },
    "authors": [
        {
            "name": "Pyor Team",
            "email": "dev@pyor.team",
            "homepage": "http://pyor.team"
        }
    ],
    "type": "project",
    "require": {
        "php": "^7.3|^8.0",
        "ext-curl": "*",
        "ext-gd": "*",
        "ext-json": "*",
        "ext-zip": "*",
        "blackcms/assets": "^1.0.0",
        "blackcms/blog": "^1.0.0",
        "blackcms/core": "^1.0.0",
        "blackcms/packages": "^1.0.0",
        "blackcms/pyoris": "^1.0.0",
        "blackcms/language": "^1.0.0",
        "blackcms/language-advanced": "^1.0.0",
        "doctrine/dbal": "^3.1",
        "laravel/framework": "9.18.0",
        "league/flysystem-aws-s3-v3": "^3.0",
        "oomphinc/composer-installers-extender": "^2.0",
        "wikimedia/composer-merge-plugin": "^2.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.16",
        "friendsofphp/php-cs-fixer": "^3.8",
        "laravel/sail": "^1.10",
        "laravel/tinker": "^2.6",
        "nunomaduro/collision": "^6.1",
        "phpunit/phpunit": "^9.0",
        "spatie/laravel-ignition": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "installer-types": ["core", "packages", "addon", "theme"],
        "installer-paths": {
            "./core": ["type:core"],
            "./packages": ["type:packages"],
            "./addons/{$name}/": ["type:addon"],
            "./themes/{$name}/": ["type:theme"]
        },
        "laravel": {
            "dont-discover": []
        },
        "merge-plugin": {
            "include": [
                "./addons/*/composer.json",
                "./themes/*/composer.json"
            ],
            "recurse": false,
            "replace": false,
            "ignore-duplicates": false,
            "merge-dev": false,
            "merge-extra": false,
            "merge-extra-deep": false,
            "merge-scripts": false
        }
    },
    "scripts": {
        "check-cs": [
            "./vendor/bin/phpcs"
        ],
        "fix-cs": [
            "./vendor/bin/phpcbf"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "wikimedia/composer-merge-plugin": true,
            "composer/installers": true,
            "oomphinc/composer-installers-extender": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
